# Google Analytics
# Get your Measurement ID from https://analytics.google.com/
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Google AdSense
# Get your AdSense ID from https://adsense.google.com/
NEXT_PUBLIC_ADSENSE_ID=ca-pub-XXXXXXXXXXXXXXXX

# Contact Email
# Email address displayed in the footer
NEXT_PUBLIC_CONTACT_EMAIL=clashconverter@gmail.com

# DNS Configuration
# Enable/disable DNS configuration in generated Clash YAML (default: true)
# Set to 'false' to disable DNS section in output
NEXT_PUBLIC_ENABLE_DNS_CONFIG=true

# Format Conversion Features
# Enable/disable specific conversion format outputs (default: true)
# Set to 'false' to hide the corresponding conversion option in UI
# NOTE: In development with Next.js 16 + Turbopack, use .env.local instead
NEXT_PUBLIC_ENABLE_CLASH_PREMIUM_TRANSFER=true
NEXT_PUBLIC_ENABLE_CLASH_META_TRANSFER=true
NEXT_PUBLIC_ENABLE_SINGBOX_TRANSFER=true
NEXT_PUBLIC_ENABLE_LOON_TRANSFER=true

# Development Override (Optional)
# Create .env.local for local development overrides
# This file takes precedence over .env and is gitignored
# Example .env.local:
# NEXT_PUBLIC_ENABLE_SINGBOX_TRANSFER=false
# NEXT_PUBLIC_ENABLE_LOON_TRANSFER=false

# Note: Create a .env.local file with your actual values for local development
# The .env.local file should not be committed to git
